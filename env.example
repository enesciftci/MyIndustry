# ===================
# MyIndustry Environment Variables
# ===================
# Copy this file to .env and update the values

# Environment
ASPNETCORE_ENVIRONMENT=Production
TAG=latest

# ===================
# PostgreSQL
# ===================
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=myindustry
POSTGRES_PORT=5432

# ===================
# Redis
# ===================
REDIS_PASSWORD=your_redis_password_here
REDIS_PORT=6379

# ===================
# RabbitMQ
# ===================
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=your_rabbitmq_password_here
RABBITMQ_PORT=5672
RABBITMQ_MANAGEMENT_PORT=15672

# ===================
# Application Ports
# ===================
API_PORT=5000
IDENTITY_PORT=5001
GATEWAY_PORT=5002

# ===================
# URLs
# ===================
IDENTITY_URL=http://myindustry-identity:8080
